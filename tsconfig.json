{
  "compilerOptions": {
    "outDir": "./built",
    "allowJs": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "module": "commonjs",
    "target": "es6"
    },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules"
  ],
  "paths": {
    "api/*": ["api/*"],
    "config/*": ["config/*"]
  }
}
